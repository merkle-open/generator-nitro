{
  "name": "nitro",
  "version": "0.0.0",
  "description": "Nitro is a Node.js application for simple and complex frontend development with a tiny footprint. This app is based on generator-nitro <%= version %>",
  "license": "UNLICENSED",
  "private": true,
  "author": "The Nitro Team",
  "engines": {
    "node": ">=0.12 <5",
    "npm": ">=1.3 <4"
  },
  "scripts": {
    "githooks": "gulp sync-githooks",
    "postinstall": "bower install && npm run githooks",
    "test": "karma start --single-run --no-auto-watch",
    "start": "node server"
  },
  "keywords": [
    "frontend",
    "app",
    "nitro",
    "component",
    "pattern",
    "modular"
  ],
  "dependencies": {
    "autoprefixer": "6.5.1",
    "body-parser": "1.15.2",
    "browser-sync": "2.17.5",
    "compression": "1.6.2",
    "del": "2.2.2",
    "dot-object": "1.5.4",
    "es6-promise": "4.0.5",
    "express": "4.14.0",
    "extend": "3.0.0",
    "globby": "6.0.0",
    "gulp": "3.9.1",
    "gulp-cached": "1.1.0",<% if (options.clientTpl) { %>
    "gulp-change": "1.0.0",<% } %>
    "gulp-concat": "2.6.0",
    "gulp-cssnano": "2.1.2",<% if (options.clientTpl) { %>
    "gulp-declare": "0.3.0",
    "gulp-handlebars": "4.0.0",<% } %>
    "gulp-header": "1.8.8",
    "gulp-imagemin": "2.4.0",<% if (options.js !== 'ESNext') { %>
    "gulp-jshint": "2.0.2",<% } if (options.pre === 'less') { %>
    "gulp-less": "3.1.0",<% } %>
    "gulp-live-server": "0.0.30",
    "gulp-load-plugins": "1.4.0",
    "gulp-newer": "1.3.0",
    "gulp-plumber": "1.1.0",
    "gulp-postcss": "6.2.0",
    "gulp-remember": "0.3.1",
    "gulp-rename": "1.2.2",<% if (options.pre === 'scss') { %>
    "gulp-sass": "2.3.2",<% } %>
    "gulp-size": "2.1.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-stylelint": "3.4.0",<% if (options.js === 'TypeScript') { %>
    "typescript": "1.5.0-beta",
    "gulp-typescript": "2.7.5",<% } else if (options.js === 'ESNext') { %>
    "babel-preset-es2015-script": "^1.1.0",
    "gulp-babel": "^6.1.2",
    "@namics/eslint-config": "^2.0.0",
    "gulp-eslint": "^3.0.1",<% } %>
    "gulp-uglify": "2.0.0",<% if (options.clientTpl) { %>
    "gulp-wrap": "0.13.0",<% } %>
    "hbs": "4.0.0",
    "hbs-utils": "0.0.3",
    "i18next": "1.10.6",
    "imagemin-pngquant": "4.2.2",
    "jasmine-core": "2.5.2",
    "jshint": "2.9.4",
    "jshint-stylish": "2.2.1",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.0.2"<% if (options.clientTpl) { %>,
    "merge-stream": "1.0.0"<% } %><% if (options.exporter) { %>,
    "nitro-exporter": "0.1.2"<% } %><% if (options.release) { %>,
    "nitro-release": "0.1.0"<% } %>
  },
  "devDependencies": {
    "gulp-watch": "4.3.10"
  }
}
