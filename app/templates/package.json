{
  "name": "nitro",
  "version": "0.0.0",
  "description": "Nitro is a Node.js application for simple and complex frontend development with a tiny footprint. This app is based on generator-nitro <%= version %>",
  "license": "UNLICENSED",
  "private": true,
  "author": "The Nitro Team",
  "engines": {
    "node": ">=4 <7",
    "npm": ">=3 <4"
  },
  "scripts": {
    "githooks": "gulp sync-githooks",
    "lint:js": "./node_modules/.bin/eslint ./patterns ./assets/js || true",
    "postinstall": "npm run githooks",
    "test": "karma start --single-run --no-auto-watch",
    "start": "node server"
  },
  "keywords": [
    "frontend",
    "app",
    "nitro",
    "component",
    "pattern",
    "modular"
  ],
  "dependencies": {<% if (options.clientTpl) { %>
    "handlebars": "4.0.6",<% } %>
    "jquery": "3.1.1",
    "terrific": "3.0.0"
  },
  "devDependencies": {
    "@namics/eslint-config": "2.0.0",
    "autoprefixer": "6.5.3",
    "babel-preset-es2015": "6.18.0",
    "body-parser": "1.15.2",
    "browser-sync": "2.18.2",
    "compression": "1.6.2",
    "del": "2.2.2",
    "dot-object": "1.5.4",
    "es6-promise": "4.0.5",
    "express": "4.14.0",
    "extend": "3.0.0",
    "globby": "6.1.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-cached": "1.1.1",<% if (options.clientTpl) { %>
    "gulp-change": "1.0.0",<% } %>
    "gulp-concat": "2.6.1",
    "gulp-cssnano": "2.1.2",<% if (options.clientTpl) { %>
    "gulp-declare": "0.3.0",<% } %>
    "gulp-eslint": "3.0.1",<% if (options.clientTpl) { %>
    "gulp-handlebars": "4.0.0",<% } %>
    "gulp-header": "1.8.8",
    "gulp-imagemin": "3.1.1",<% if (options.pre === 'less') { %>
    "gulp-less": "3.3.0",<% } %>
    "gulp-live-server": "0.0.30",
    "gulp-load-plugins": "1.4.0",
    "gulp-newer": "1.3.0",
    "gulp-plumber": "1.1.0",
    "gulp-postcss": "6.2.0",
    "gulp-remember": "0.3.1",
    "gulp-rename": "1.2.2",<% if (options.pre === 'scss') { %>
    "gulp-sass": "2.3.2",<% } %>
    "gulp-size": "2.1.0",
    "gulp-sourcemaps": "2.2.1",
    "gulp-stylelint": "3.4.0",<% if (options.js === 'TypeScript') { %>
    "typescript": "1.5.0-beta",
    "gulp-typescript": "2.7.5", <% } %>
    "gulp-uglify": "2.0.0",
    "gulp-watch": "4.3.11",<% if (options.clientTpl) { %>
    "gulp-wrap": "0.13.0",<% } %>
    "hbs": "4.0.1",
    "hbs-utils": "0.0.4",
    "i18next": "4.1.0",
    "i18next-express-middleware": "1.0.2",
    "i18next-node-fs-backend": "0.1.3",
    "i18next-sprintf-postprocessor": "^0.2.2",
    "imagemin-pngquant": "5.0.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.0.2"<% if (options.clientTpl) { %>,
    "merge-stream": "1.0.0"<% } %><% if (options.exporter) { %>,
    "nitro-exporter": "0.1.2"<% } %><% if (options.release) { %>,
    "nitro-release": "0.1.0"<% } %>
  }
}
