<a class="a-cta{{#if _class}} {{_class}}{{/if}}" href="{{href}}">
	<div class="a-cta__icon">
		{{pattern name='icon' icon=icon}}
	</div>
	<div class="a-cta__circle a-cta__circle--size-s"></div>
	<div class="a-cta__circle a-cta__circle--size-m"></div>
	<div class="a-cta__circle a-cta__circle--size-l"></div>
	<span class="a-cta__text">{{text}}</span>
</a>
