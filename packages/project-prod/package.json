{
  "name": "nitro-project-prod",
  "version": "0.0.0",
  "description": "Nitro is a Node.js application for simple and complex frontend development with a tiny footprint. This app is based on generator-nitro 5.0.0",
  "license": "UNLICENSED",
  "private": true,
  "author": "The Nitro Team",
  "engines": {
    "node": ">=10.15.0 <13",
    "npm": ">=6.4.1 <7"
  },
  "scripts": {
    "check-node-version": "check-node-version --print --package",
    "prod": "npm-run-all prod:*",
    "prod:check": "npm run check-node-version",
    "prod:serve": "cross-env NODE_ENV=production nitro-app-serve",
    "start": "npm run prod"
  },
  "keywords": [
    "frontend",
    "app",
    "nitro",
    "component",
    "pattern",
    "modular"
  ],
  "dependencies": {
    "@nitro/app": "*",
    "check-node-version": "4.0.1",
    "config": "3.2.4",
    "cross-env": "6.0.3",
    "extend": "3.0.2",
    "npm-run-all": "4.1.5",
    "npm-check-updates": "3.2.2",
    "rimraf": "3.0.0"
  },
  "devDependencies": {}
}
