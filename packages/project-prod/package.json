{
  "name": "nitro-project-prod",
  "version": "0.0.0",
  "description": "Nitro is a Node.js application for simple and complex frontend development with a tiny footprint. This app is based on generator-nitro 10.0.0",
  "license": "UNLICENSED",
  "private": true,
  "author": "The Nitro Team",
  "engines": {
    "node": ">=20.18.1 <21",
    "npm": ">=10.8.2 <11"
  },
  "scripts": {
    "check-node-version": "check-node-version --print --package",
    "prod": "npm-run-all prod:*",
    "prod:check": "npm run check-node-version",
    "prod:serve": "cross-env NODE_ENV=production nitro-app-serve",
    "start": "npm run prod"
  },
  "keywords": [
    "frontend",
    "app",
    "nitro",
    "component",
    "pattern",
    "modular"
  ],
  "dependencies": {
    "@nitro/app": "*",
    "check-node-version": "4.2.1",
    "config": "4.1.1",
    "cookie-session": "2.1.1",
    "cross-env": "10.1.0",
    "extend": "3.0.2",
    "hbs": "4.2.0",
    "i18next": "25.5.3",
    "npm-run-all": "4.1.5"
  }
}
